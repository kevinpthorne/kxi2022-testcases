// + means test passed, | separates tests
// if any - is found, test failed
// other symbols like . or , maybe used to
// show intermediate steps
void kxi2022 main() {
    int a = 0;
    switch (a) {
        case 1:
            cout << '-';
            break;
        case 0:
        case 3:
        default:
            cout << '+';
    }
    cout << '|';
    cout << '\n';


    char b = 'b';
    switch (b) {
        case 'b':
        case 'c':
            cout << '+';
            break;
        default:
            break;
    }
    cout << '|';
    cout << '\n';
}

